{
    "extends": "eslint-config-rackt",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
    },
    "rules": {
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/jsx-no-undef": 2
    },
    "plugins": [
      "react"
    ]
}
  